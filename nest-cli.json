{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/beckn-domain-adapters/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/beckn-domain-adapters/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/beckn-domain-adapters",
  "projects": {
    "beckn-domain-adapters": {
      "type": "application",
      "root": "apps/beckn-domain-adapters",
      "entryFile": "main",
      "sourceRoot": "apps/beckn-domain-adapters/src",
      "compilerOptions": {
        "tsConfigPath": "apps/beckn-domain-adapters/tsconfig.app.json"
      }
    },
    "bg": {
      "type": "application",
      "root": "apps/bg",
      "entryFile": "main",
      "sourceRoot": "apps/bg/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bg/tsconfig.app.json"
      }
    },
    "bap": {
      "type": "application",
      "root": "apps/bap",
      "entryFile": "main",
      "sourceRoot": "apps/bap/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bap/tsconfig.app.json"
      }
    },
    "bpp": {
      "type": "application",
      "root": "apps/bpp",
      "entryFile": "main",
      "sourceRoot": "apps/bpp/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bpp/tsconfig.app.json"
      }
    }
  }
}